           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="shq_aws_ses_monitor.entity_manager" class="Doctrine\ORM\EntityManager">
            <factory service="doctrine" method="getManager" />
            <argument>%shq_aws_ses_monitor.model_manager_name%</argument>
        </service>
    </services>
</container>
