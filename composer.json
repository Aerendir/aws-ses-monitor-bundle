{
    "name": "serendipity_hq/bundle-aws-ses-monitor",
    "type": "symfony-bundle",
    "description": "Symfony bundle to monitor AWS SES bounces, complaints and deliveries through SNS. Comes with a command to fully configure the topics and includes configurable SwiftMailer filter to avoid sending emails to bounced and complained emails.",
    "keywords": ["aws", "ses", "sns", "bounce", "complaints", "blacklist", "swiftmailer"],
    "homepage": "https://github.com/Aerendir/bundle-aws-ses-monitor",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Aerendir/aws-ses-monitor-bundle/issues"
    },
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php-symfony": "^2.0",
        "doctrine/common": "^2.4",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.5",
        "serendipity_hq/console-styles-bundle": "dev-dev",
        "symfony/framework-bundle": "^3.0|^4.0|^5.0",
        "symfony/http-kernel": "^3.0|^4.0|^5.0",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/yaml": "^3.0|^4.0|^5.0",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "drenso/phan-extensions": "@stable",
        "friendsofphp/php-cs-fixer": "^2.0",
        "phan/phan": "^3.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-doctrine": "@stable",
        "phpstan/phpstan-phpunit": "@stable",
        "phpstan/phpstan-symfony": "@stable",
        "rector/rector": "^0.7",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^3.4|^4.1|^6.0",
        "serendipity_hq/phpunit_helper": "*",
        "symfony/console": "^3.0|^4.0|^5.0",
        "symfony/phpunit-bridge": "^3.0|^4.0|^5.0",
        "symfony/var-dumper": "^3.0|^4.0|^5.0",
        "thecodingmachine/phpstan-safe-rule": "@stable",
        "vimeo/psalm": "^3.12",
        "weirdan/doctrine-psalm-plugin": "@stable"
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\AwsSesMonitorBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\AwsSesMonitorBundle\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "nikic/php-parser": "4.6.0"
    }
}
