{
    "name": "serendipity_hq/aws-ses-monitor-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to monitor AWS SES bounces, complaints and deliveries through SNS. Comes with a command to fully configure the topics and includes configurable SwiftMailer filter to avoid sending emails to bounced and complained emails.",
    "keywords": ["aws", "ses", "sns", "bounce", "complaints", "blacklist", "swiftmailer"],
    "homepage": "https://github.com/Aerendir/aws-ses-monitor-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Aerendir/aws-ses-monitor-bundle/issues"
    },
    "authors": [
        {
            "name": "Audrius Karabanovas",
            "email": "audrius@karabanovas.net"
        },
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "aws/aws-php-sns-message-validator": "^1.5",
        "aws/aws-sdk-php": "^3.52",
        "aws/aws-sdk-php-symfony": "^2.0",
        "doctrine/common": "^2.4",
        "doctrine/doctrine-bundle": "^1.7",
        "doctrine/orm": "^2.5",
        "serendipity_hq/console-styles-bundle": "^1.1",
        "symfony/framework-bundle": "^3.4|^4.1",
        "symfony/http-kernel": "^3.4|^4.1",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/yaml": "^3.4|^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "roave/security-advisories":"dev-master",
        "sensiolabs/security-checker": "^5.0.3",
        "serendipity_hq/phpunit_helper": "*",
        "symfony/console": "^3.4|^4.1",
        "symfony/var-dumper": "^3.4|^4.1",
        "symfony/phpunit-bridge": "^3.4|^4.1"
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Bundle\\AwsSesMonitorBundle\\": "src"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    }
}
